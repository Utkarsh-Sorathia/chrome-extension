{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "A Chrome extension with Bootstrap",
    "action": {
      "default_popup": "UI/popup.html",
      "default_icon": "icons/icon128-react.png"
    },
    "icons": {
      "16": "icons/icon16-react.png",
      "48": "icons/icon48-react.png",
      "128": "icons/icon128-react.png"
    },
    "content_security_policy": {
      "script-src": "'self' https://cdn.jsdelivr.net",
      "object-src": "'self'"
    },
    "permissions": ["storage", "tabs"]
  }
  